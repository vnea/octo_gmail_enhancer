{
  "name": "OCTO Gmail enhancer",
  "version": "1.0.2",
  "description": "Cette extension permet de simplifier l'affichage du Gmail d'OCTO/ACN en supprimant le \"bruit\"",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "arrive-2.4.1.min.js",
        "main.js"
      ]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  }
}
